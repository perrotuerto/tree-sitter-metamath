{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "metamath",
      "camelcase": "Metamath",
      "title": "Metamath",
      "scope": "source.metamath",
      "file-types": [
        "mm"
      ],
      "injection-regex": "^metamath$",
      "class-name": "TreeSitterMetamath"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "GPL-2.0",
    "description": "A simple and flexible computer-processable language that supports rigorously verifying, archiving, and presenting mathematical proofs.",
    "authors": [
      {
        "name": "Ramiro Santa Ana Anguiano",
        "email": "hi@colima.press"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-metamath"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
